{
  "functions": [
    {
      "source": "functions-python-simple",
      "codebase": "python-scraper-simple",
      "runtime": "python311"
    }
  
  ],
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
